@model CarTrade.Models.AutoView

@{
    ViewBag.Title = "Details";
}


<fieldset>
    <legend>Auto</legend>
    <h4> @Model.marka, @Model.tipus</h4>
    <h4>@Model.evjarat</h4>
    <hr />
    <br/>
    <table>
        <tr>
            <td><b>Gyártási idő:</b></td>
            <td><b>@Model.gyartasiIdo</b></td>
        </tr>
        <tr>
            <td><b>Előző tulajdonosok száma:</b></td>
            <td><b>@Model.tulajdonosokSzama</b></td>
        </tr>
        
        <tr>
            <td><b>Állapot:</b></td>
            <td><b>@Model.allapot</b></td>
        </tr>
        <tr><td colspan="2"></td></tr>
        <tr>
            <td><b>Elérhetőség:</b></td>
            <td><b>@Model.telephely</b></td>
        </tr>
    </table>
    
 </fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
